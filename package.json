{
  "name": "councils",
  "version": "1.0.0",
  "description": "An church calling productivity tool",
  "scripts": {
    "start": "NODE_ENV=development node node_modules/.bin/webpack-dev-server --hot --inline",
    "test-ci": "NODE_ENV=test ./node_modules/karma/bin/karma start ./karma.conf.js --single-run --browsers PhantomJS,Firefox",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start ./karma.conf.js --single-run",
    "tdd": "NODE_ENV=test ./node_modules/karma/bin/karma start ./karma.conf.js --browsers Chrome",
    "eslint": "node_modules/.bin/eslint src/**",
    "coveralls": "npm run test-ci && node node_modules/lcov-filter/index.js ./coverage/phantomjs/lcov.info spec | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "rm -rf ./www && NODE_ENV=production ./node_modules/.bin/webpack",
    "build-native": "rm -rf ./www && NODE_ENV=production ./node_modules/.bin/webpack && cordova build",
    "preversion": "npm run eslint && npm test",
    "version": "npm run build && npm run changelog && git add -A CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "ios": "cordova emulate ios",
    "android": "cordova run android"
  },
  "cordovaPlatforms": [
    "android",
    "ios"
  ],
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "com.ionic.keyboard",
    {
      "locator": "https://github.com/phonegap-build/PushPlugin.git",
      "id": "com.phonegap.plugins.PushPlugin"
    },
    "cordova-plugin-wkwebview-engine"
  ],
  "author": "Nick Randall <nick.randall@domo.com>",
  "repository": "https://github.com/councils/councils",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015-loose": "^6.1.3",
    "babel-preset-stage-1": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.1.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.26.2",
    "glob": "^6.0.1",
    "html-webpack-plugin": "^1.7.0",
    "image-webpack-loader": "^1.6.2",
    "install": "^0.4.0",
    "istanbul": "^0.4.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "lcov-filter": "0.1.1",
    "mocha": "^2.3.4",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-sass": "^3.4.2",
    "npm": "^3.5.0",
    "phantomjs": "^1.9",
    "portfinder": "^0.4.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-reporter": "^1.3.0",
    "precss": "^1.3.0",
    "raw-loader": "^0.5.1",
    "request": "^2.67.0",
    "ryuu-client": "^2.5.0",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-material": "^0.11.4",
    "angular-moment": "^0.10.3",
    "angular-sanitize": "^1.4.8",
    "angular-ui-router": "^0.2.15",
    "angularfire": "^1.1.3",
    "babel-polyfill": "^6.2.0",
    "firebase": "^2.3.2",
    "ionic": "^1.7.11",
    "ionic-service-analytics": "^0.2.5",
    "ionic-service-core": "^0.1.10",
    "ionic-service-push": "^0.1.13",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "ng-cordova": "^0.1.23-alpha"
  }
}
